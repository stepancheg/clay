import printer.(println);
import data.strings.*;
import data.sequences.operators.*;
import test.*;

main() = testMain(TestSuite("string literals", array(
        TestCase("stringLiteralByteSlice", -> {
            var foo = #stringLiteralByteSlice(unwrapStatic("foobar"), 0, 3);
            var bar = #stringLiteralByteSlice(unwrapStatic("foobar"), 3, 6);
            expectEqual("foo", foo);
            expectEqual("bar", bar);
            expectEqual("foobar", foo ++ bar);
        }),
    )));

